@model CarService.Models.Entities.TblJobRemarkMaster

@{
    ViewData["Title"] = "Create";
}

<h1>Create Remark</h1>

<hr />

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="FldRemarkTitle" class="control-label"></label>
                <input asp-for="FldRemarkTitle" class="form-control" />
                <span asp-validation-for="FldRemarkTitle" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create Remark" class="btn btn-primary" />
                <a class="btn btn-outline-primary" asp-action="Index">Back to List</a>
            </div>
        </div>
    </div>
</form>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
